//1. Define a function to check whether a given number is a Prime number or not.

#include<iostream>
using namespace std;
void prime(int n);
int  main()

{
    prime( -2);
}

void prime(int n)
{   int flag=0;

    
    
    for (int i=2; i<=n/2; i++)
        {
            
            if(n%i==0)
               {cout<<"not prime";
                   flag=1;
                   break;
                   
               }
           
        }
    if(flag==0)
           { cout<<n<<" is prime";}
        
}
